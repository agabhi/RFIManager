<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

 <hibernate-mapping>

	<class name="com.model.rfi.RFIApproval" table="rfi_approval">
	
		<id name="id" column="id">
	    	<generator class="native"/>
	    </id>
	    
	    <property name="approvedFrom" column="approved_from"/>
		<property name="approvedTo" column="approved_to"/>
		   <property name="approvedSide">
   	<column name="approved_side"/>
   </property>
   
   <property name="approvedLayer">
   	<column name="approved_layer"/>
   </property>
  <property name="approvalDate">
   	<column name="approval_date"/>
   </property>
   
   <property name="breadth">
   	<column name="breadth"/>
   </property>
   
   <property name="depth">
   	<column name="depth"/>
   </property>
   
   <property name="area">
   	<column name="area"/>
   </property>
   
   <property name="quantity">
   	<column name="quantity"/>
   </property>

	</class>

 </hibernate-mapping>